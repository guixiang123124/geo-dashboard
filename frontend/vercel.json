{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://geo-insights-api-production.up.railway.app/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://geo-insights-api-production.up.railway.app/health"
    }
  ]
}
